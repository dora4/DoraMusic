<?xml version="1.0" encoding="utf-8"?>
<layer-list
        xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#0E000000"/>
            <stroke android:width="0dp" android:color="@color/white" android:dashWidth="0dp"
                    android:dashGap="0dp"/>
            <corners android:topLeftRadius="1dp" android:topRightRadius="1dp"
                    android:bottomLeftRadius="1dp" android:bottomRightRadius="1dp"/>
            <padding android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="0dp"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="#27000000"/>
                <stroke android:width="0dp" android:color="@color/white" android:dashWidth="0dp"
                        android:dashGap="0dp"/>
                <corners android:topLeftRadius="1dp" android:topRightRadius="1dp"
                        android:bottomLeftRadius="1dp" android:bottomRightRadius="1dp"/>
                <padding android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="0dp"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/white"/>
                <stroke android:width="0dp" android:color="@color/white" android:dashWidth="0dp"
                        android:dashGap="0dp"/>
                <corners android:topLeftRadius="1dp" android:topRightRadius="1dp"
                         android:bottomLeftRadius="1dp" android:bottomRightRadius="1dp"/>
                <padding android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="0dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
